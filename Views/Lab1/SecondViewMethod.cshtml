@model List<string>

<h2>Отсортированный список овощей</h2>

<table class="table" style="width:20%">
    <thead>
        <tr>
            <th class="text-center">Наименование</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var value in Model)
        { 
            <tr class="text-center">
                <td>@value</td>
            </tr>
        }
    </tbody>
</table>

